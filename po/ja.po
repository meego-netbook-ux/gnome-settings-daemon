#: ../data/apps_gnome_settings_daemon_keybindings.schemas.in.h:24
msgid "Launch help browser"
msgstr "ヘルプ・ブラウザを起動する"

#: ../data/apps_gnome_settings_daemon_keybindings.schemas.in.h:25
msgid "Launch media player"
